<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="Entity">
    <class name="Person" table="[TelefonView]">
        <id name="ID" column="ID">
            <generator class="native"/>
        </id>
        <property name="FIO" column="ФИО"/>
        <property name="telefonNum" column="ТелефонКод"/>
        <property name="room" column="Номер_кабинета"/>
        <property name="profsession" column="Должность"/>
        <property name="otdel" column="Отдел"/>
    </class>
</hibernate-mapping>